{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)/save",
      "dest": "/api/$1/save.php"
    },
    {
      "src": "/(.*)/delete",
      "dest": "/api/$1/delete.php"
    },
    {
      "src": "/login.php",
      "dest": "/api/login.php"
    },
    {
      "src": "/logout.php",
      "dest": "/api/logout.php"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php?url=$1"
    }
  ]
}
